plugins {
    id 'nf-amazon'
}

profiles {
    local {
        process.executor = 'local'
    }


    awscloud {
        process {
            executor = 'awsbatch'
            queue = 'nextflow-job-queue-demo'
            container = 'amazonlinux'

        }

        aws {
            region = 'ap-southeast-1'
            batch {
                cliPath = '/usr/local/bin/aws'
            }

        }
    }

}


