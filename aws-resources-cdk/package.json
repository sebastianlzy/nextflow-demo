{
  "name": "aws-resources-cdk",
  "version": "0.1.0",
  "bin": {
    "aws-resources-cdk": "bin/aws-resources-cdk.js"
  },
  "scripts": {
    "build": "echo \"The build step is not required when using JavaScript!\" && exit 0",
    "cdk": "cdk",
    "test": "jest",
    "cdk:synth": "cdk synth > ./cdk.out/nextflow-demo.template.yaml",
    "cdk:deploy": "cdk bootstrap; cdk synth; cdk deploy --outputs-file ../aws-outputs.json",
    "install-prerequisite": ". ./install-prequisite.sh"
  },
  "devDependencies": {
    "jest": "^26.4.2"
  },
  "dependencies": {
    "@aws-cdk/aws-batch": "^1.201.0",
    "@aws-cdk/aws-cloud9": "^1.201.0",
    "@aws-cdk/aws-ec2": "^1.201.0",
    "@aws-cdk/aws-ecs": "^1.201.0",
    "@aws-cdk/aws-iam": "^1.201.0",
    "@aws-cdk/aws-s3": "^1.201.0",
    "@aws-cdk/core": "^1.201.0"
  }
}
